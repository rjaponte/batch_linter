{
  "name": "batch-linter",
  "version": "1.0.0",
  "description": "Testing amp-toolbox linter package in loop",
  "main": "main.js",
  "scripts": {
    "postinstall": "",
    "build": "tsc --resolveJsonModule --module commonjs --target es2018 --esModuleInterop --strictNullChecks --declaration --outDir dist *.ts",
    "transpile": "tsc --resolveJsonModule --module commonjs --target es2018 --esModuleInterop --noImplicitAny --strictNullChecks src/*.ts tests/*.ts",
    "start": "ts-node main.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/date-and-time": "^0.13.0",
    "@types/node": "^12.8.3",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "@ampproject/toolbox-linter": "^2.6.0",
    "chalk": "^4.1.0",
    "cheerio": "^1.0.0-rc.3",
    "commander": "^6.1.0",
    "csv-reader": "^1.0.7",
    "csv-writer": "^1.6.0",
    "date-and-time": "^0.14.1",
    "events": "^3.2.0",
    "fs": "0.0.1-security",
    "googleapis": "^39.2.0",
    "node": "12.18.4",
    "node-fetch": "^2.6.7",
    "readline": "^1.3.0",
    "ts-node": "^9.0.0"
  }
}
